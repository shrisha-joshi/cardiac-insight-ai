{
  "metadata": {
    "name": "UCI Heart Disease Dataset",
    "version": "1.0.0",
    "source": "UCI Machine Learning Repository",
    "url": "https://archive.ics.uci.edu/ml/datasets/heart+disease",
    "records": 303,
    "features": 13,
    "target": "Heart Disease Presence (0=Absence, 1=Presence)",
    "description": "This dataset contains 303 patient records from UCI with 13 clinical parameters used to predict heart disease presence",
    "createdDate": "2025-11-04",
    "accuracy": 0.857,
    "aucroc": 0.918,
    "trainTestSplit": "80/20"
  },
  "features": [
    {
      "name": "age",
      "description": "Age in years",
      "type": "numeric",
      "range": [29, 77],
      "mean": 54.3,
      "std": 9.1,
      "unit": "years"
    },
    {
      "name": "sex",
      "description": "Sex (1=Male, 0=Female)",
      "type": "categorical",
      "values": [0, 1],
      "mean": 0.683,
      "std": 0.467,
      "interpretation": "1=Male (higher risk), 0=Female"
    },
    {
      "name": "cp",
      "description": "Chest pain type",
      "type": "categorical",
      "values": [1, 2, 3, 4],
      "mean": 2.75,
      "std": 1.05,
      "interpretation": "1=Typical angina, 2=Atypical angina, 3=Non-anginal pain, 4=Asymptomatic"
    },
    {
      "name": "trestbps",
      "description": "Resting blood pressure",
      "type": "numeric",
      "range": [94, 200],
      "mean": 131.6,
      "std": 17.5,
      "unit": "mmHg"
    },
    {
      "name": "chol",
      "description": "Serum cholesterol level",
      "type": "numeric",
      "range": [126, 564],
      "mean": 246.3,
      "std": 51.8,
      "unit": "mg/dL"
    },
    {
      "name": "fbs",
      "description": "Fasting blood sugar > 120 mg/dL",
      "type": "categorical",
      "values": [0, 1],
      "mean": 0.145,
      "std": 0.353,
      "interpretation": "1=True (elevated), 0=False (normal)"
    },
    {
      "name": "restecg",
      "description": "Resting electrocardiographic results",
      "type": "categorical",
      "values": [0, 1, 2],
      "mean": 0.99,
      "std": 0.89,
      "interpretation": "0=Normal, 1=ST-T abnormality, 2=LVH (left ventricular hypertrophy)"
    },
    {
      "name": "thalach",
      "description": "Maximum heart rate achieved",
      "type": "numeric",
      "range": [60, 202],
      "mean": 149.6,
      "std": 23.2,
      "unit": "bpm"
    },
    {
      "name": "exang",
      "description": "Exercise-induced angina",
      "type": "categorical",
      "values": [0, 1],
      "mean": 0.327,
      "std": 0.470,
      "interpretation": "1=Yes (risk factor), 0=No (protective)"
    },
    {
      "name": "oldpeak",
      "description": "ST depression induced by exercise",
      "type": "numeric",
      "range": [0, 6.2],
      "mean": 1.04,
      "std": 1.16,
      "unit": "mm (ST depression)"
    },
    {
      "name": "slope",
      "description": "Slope of ST segment",
      "type": "categorical",
      "values": [1, 2, 3],
      "mean": 1.60,
      "std": 0.614,
      "interpretation": "1=Upsloping (good), 2=Flat, 3=Downsloping (concerning)"
    },
    {
      "name": "ca",
      "description": "Number of major vessels colored by fluoroscopy",
      "type": "numeric",
      "range": [0, 4],
      "mean": 0.729,
      "std": 1.02,
      "unit": "vessels"
    },
    {
      "name": "thal",
      "description": "Thalassemia/Blood supply defect",
      "type": "categorical",
      "values": [3, 6, 7],
      "mean": 4.73,
      "std": 1.93,
      "interpretation": "3=Normal, 6=Fixed defect, 7=Reversible defect"
    }
  ],
